<?xml version='1.0' encoding='utf-8'?>
<widget id="br.fib.provadois" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Prova Dois</name>
    <description>
        Prova de Linguagem de Programação V
    </description>
    <author email="marco_oliveira94@live.com" href="http://www.fibbauru.br">
        Marco Antonio de Oliveira Junior
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
	<preference name="StatusBarBackgroundColor" value="#E86C13" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="~4.2.1" />
    <engine name="android" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.0" />
</widget>
